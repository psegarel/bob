@linei:@template"*/* This file will hold styles for the mobile version of your website (mobile first). */
/* This also can include ANY global CSS that applies site-wide. Unless overwritten by a more specific style rule, CSS declarations in global.css will apply site-wide. */
@import 'base';


/*------------------------------ TYPOGRAPHY ------------------------------------------------------------*/
body												{	font-family: 'PT Sans Narrow', sans-serif;
														font-size: $lead * .8;
													}

h2 , h3 , h4 , h5 , h6								{	@include title_family;
													}

h1.site-name a,
h2.site-name a										{	font-family: Lobster, Georgia , serif; 
														color: $medium; 
														text-shadow: none; 
														line-height: 80%; 
														letter-spacing: 1px; 
														text-transform: none;
														padding-right: 4em;
													}			
		
#zone-user-wrapper,
#zone-content-wrapper,
#zone-footer-wrapper								{	color: #fff;
													}	

#zone-content-wrapper								{	font-size: 1.2em;
													}
						
#zone-footer-wrapper								{	font-size: $lead * .8;

														h2 , h3	{	color: $brighter;
																	font-size: $lead;
																	text-transform: uppercase;
																	border-bottom: 1px dotted $brighter;
																}
														
														a 	{ 	color: $light;
															}	
													}

h2.block-title										{	font-size: $lead*1.5;
														color: $darkest;
													}
													
h6.site-slogan										{	color: $light;
														text-transform: none;
													}
													
.sassy												{	@include sassy-button('shiny', 10px , 12px, $bright);
														a 	{ 	text-decoration: none;
																color: white;
															}
													}

#boxes-box-subtitle 								{	.boxes-box-content  	{  p 	{ color: $brighter;
																						}
																				}
													}


.smalltext											{	p 	{ font-size: $lead * .9;
															}
													}

.faded												{	p 	{ color: #777;
															}
													}
							
#block-boxes-subtitle p								{	color: #fff;
														text-transform: none;
													}

.article											{	font-size: $lead;
													}
	
.more												{	font-family: 'Century Gothic' , sans-serif;
														font-size: 11px;
														text-transform: uppercase;
														
														a 	{ 	color: $light;
																text-decoration: none;
																
																&:hover {	text-decoration: underline;
																		}
															}
													}

#block-views-footer-get-in-touch					{	h4	{	padding-top: 4px;
																margin: 0;
																line-height: 1;
																color: $brighter;
																padding-bottom: 10px;
															}
															
														.dotted-top	{	border-bottom: 1px dotted $brighter;
																		padding-top: 8px;
																		margin-bottom: 10px;
																	}	
															
														img	{	margin-bottom: 40px;
															}	
													}								

#block-views-front-page-items-all-of-me,
#block-views-references-front-reference				{	font-size: $lead;
														line-height: 120%; 
														
														h3	{ 	font-weight: bold;
																font-size: 2em;
																color: $darkest;
																margin-top: 20px;
														}
														
														img	{	display: none;
															}
													}

#block-views-footer-social-networks					{	margin-bottom: 2em;
													}
													
#block-views-pages-about-block						{	.adaptive-image	{	margin-bottom: 20px;
																		}
													}
								
#block-views-quote-frontpage-quote 					{ 	padding: 0 0 20px 0;
														margin-bottom: 80px;
		
														.quote-body p,
														.quote-author {		font-family: Lobster, Georgia , serif; 
																			font-size: $lead * 2.4; 
																			line-height: 120%;
																		}
		
														.quote-body p 	{	margin: 2em 0 0 2em; 
																			color: $medium;
																		}
		
														.quote-author 	{	padding: 0;
																			margin: 0; 
																			color: $darkest;
																			margin: 1em 0 0 2em;
																		}
													}
													
#block-views-news-front-news						{	h2.block-title	{	color: $medium;
																			position: absolute;
																			top: -42%;
																			left: 33%;
																			text-transform: uppercase;
																			font-size: 70px;
																			font-family: 'Lobster' , sans-serif;
																			font-weight: bold;
																		}
																		
														h3				{	border-bottom: 1px dotted $bright;
																			padding: 16px 0 6px 0;
																			color: $brighter;
																		}					
													}
													
.icon-facebook-sign,
.icon-twitter,
.icon-envelope,
.icon-phone											{	margin-right: 10px;
													}


/*------------------------------ END TYPOGRAPHY ------------------------------------------------------------*/


		
