					}

#zone-branding-wrapper	{	margin: 0;
							padding: 0;
						}
					
#zone-header			{	border-top: 1px dotted $bright;
							border-bottom: 1px dotted $bright;
							padding: 20px 0;
						}
						
#zone-menu-wrapper		{	margin: 2em 0 0 0;
						}


#zone-user-wrapper		{	margin-bottom: 1em;
							background: $headerColor;
						}


#zone-header-wrapper,
#zone-preface-wrapper	{	margin-top: 0;
							margin-bottom: 4em;
						}


#zone-postscript-wrapper	{	padding: 4em 0;
