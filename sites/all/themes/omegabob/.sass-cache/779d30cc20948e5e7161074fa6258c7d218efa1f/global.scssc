/* This also can include ANY global CSS that applies site-wide. Unless overwritten by a more specific style rule, CSS declarations in global.css will apply site-wide. */
@import 'base';


/*------------------------------ TYPOGRAPHY ------------------------------------------------------------*/
body	{	font-family: 'PT Sans Narrow', sans-serif;
			font-size: $lead * .8;
		}

h2 , h3 , h4 , h5 , h6	{	@include title_family;
						}

h1.site-name a,
h2.site-name a	{	font-family: Lobster, Georgia , serif; 
					color: $medium; 
					text-shadow: none; 
					line-height: 80%; 
					letter-spacing: 1px; 
					text-transform: none;
				}			
		
#zone-user-wrapper	{	color: #fff;
					}	

#zone-content-wrapper	{	color: #fff;
							font-size: 1.2em;
						}
						
#zone-footer-wrapper	{	color: #fff;
							font-size: $lead * .8;

							h2 , h3	{	color: $brighter;
										font-size: $lead;
										text-transform: uppercase;
										border-bottom: 1px dotted $brighter;
									}
							
							a 	{ 	color: $light;
								}	
}

h2.block-title	{	font-size: $lead*1.5;
					color: $darkest;
				}


h6.site-slogan,
#block-boxes-subtitle p,
#block-views-news-front-news h2		{	color: $light;
										text-transform: none;
									}


.smalltext		{	p 	{ font-size: $lead * .9;
						}
				}


.faded			{	p 	{ color: #777;
						}
				}
								
#block-boxes-robert-brusen	{	/* color: $bright; */
							}
							
#block-boxes-subtitle p			{	color: #fff;
									text-transform: none;
								}

#block-views-pages-awards  {	margin-top: 20px;
								margin-left: -12px;

								p 	{	font-size: $lead * .9;
