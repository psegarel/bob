							padding: 2em 0;
						}


.context-news #zone-content-wrapper	{	background: transparent;
										color: $darkest;
									}

#zone-footer-wrapper	{	@include background_dark_pattern;
							background-color: $darkest;
							border-top: 10px solid $medium;
							padding: 4em 0 8em 0;
							
							h2 , h3	{	margin-bottom: 10px;
									}
							
							a 	{ 	text-decoration: none;
